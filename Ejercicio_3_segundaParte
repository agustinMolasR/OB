public class Main {

    public static void main(String[] args) {
        Coche miCoche = new Coche();
        miCoche.aumentarPuertas();
        miCoche.aumentarPuertas();

        System.out.println(miCoche.puertas);
    }
}

class Coche {
    public int puertas = 3;
    public void aumentarPuertas() {
        this.puertas++;
    }
}
